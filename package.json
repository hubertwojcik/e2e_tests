{
  "name": "E2E_tests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "detox:build:ios-debug": "detox build --configuration ios.sim.debug",
    "detox:e2e:ios-debug": "DETOX_CONFIGURATION=ios.sim.debug DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js",
    "detox:build:ios-release": "detox build --configuration ios.sim.release",
    "detox:e2e:ios-release": "DETOX_CONFIGURATION=ios.sim.release DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js",
    "detox:build:android-debug": "detox build --configuration android.emu.debug",
    "detox:e2e:android-debug": "DETOX_CONFIGURATION=android.emu.debug DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js",
    "detox:build:android-release": "detox build --configuration android.emu.release",
    "detox:e2e:android-release": "DETOX_CONFIGURATION=android.emu.release DETOX_RECORD_VIDEOS='failing' DETOX_RECORD_LOGS='failing' npx cucumber-js"
  },
